<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.yeokku.model.dao.UserPathDAO">

	<select id="selectLastUserPathId"  resultType="Integer">
		SELECT LAST_INSERT_ID();
	</select>
	
	<insert id="insertUserPath" parameterType="UserPath">
		insert into user_path_test
			(userid, save_date, country_name)
		values
			(
				#{userId},
				sysdate(),
				#{countryName}
			)
	</insert>
	
	<insert id="insertConfigPath" parameterType="ConfigPath">
		insert into config_path_test
			(pid, p_order, upid)
		values
			(
				#{pointId},
				#{pointOrder},
				#{pathId}
			)
	</insert>
	
</mapper>




















